def debug\_api\_call(prompt, config, save\_to\_file=True):  
 	"""  
 	Make an API call with detailed debugging information.  
 	"""  
 	print("üîç Debug mode: Making API call with full logging...")  
 	  
 	\# Log the request details  
 	print(f"üì§ Request URL: {config\['base\_url'\]}/chat/completions")  
 	print(f"üì§ Model: {config\['model'\]}")  
 	print(f"üì§ Temperature: {config\['temperature'\]}")  
 	print(f"üì§ Max Tokens: {config\['max\_tokens'\]}")  
 	print(f"üì§ Prompt: {prompt\[:100\]}...")  
 	  
 	\# Make the call and capture timing  
 	start\_time \= time.time()  
 	result \= call\_deepseek\_api(prompt, config)  
 	end\_time \= time.time()  
 	  
 	print(f"‚è±Ô∏è Request took: {end\_time \- start\_time:.2f} seconds")  
 	  
 	if result:  
     	print("üì• Response received successfully")  
     	if 'usage' in result:  
         	usage \= result\['usage'\]  
         	print(f"üìä Tokens used: {usage.get('total\_tokens', 'unknown')}")  
     	  
     	\# Optionally save to file for detailed analysis  
     	if save\_to\_file:  
         	debug\_filename \= f"debug\_response\_{int(time.time())}.json"  
         	with open(debug\_filename, 'w') as f:  
             	json.dump(result, f, indent=2)  
         	print(f"üíæ Full response saved to: {debug\_filename}")  
 	else:  
     	print("‚ùå No response received")  
 	  
 	return result






